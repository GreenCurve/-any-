# Получаем входные значения
C1 = [52,54,53,48,46]
C2 = [46,45,47,51,44]
C1PAC2 = [63,70,64,65,62]
C1PEC2 = [16,13,14,14,6]

Известная_Емкость = 0.25
Погрешность_Известной_Емкости = 0.025

Известное_число_делений = 33
Погрешность_Известного_числа_делений = 0

for i in [C1,C2,C1PAC2,C1PEC2]:
    значения = i

    # Вычисляем среднее значение
    среднее_значение = sum(значения) / len(значения)

    # Вычисляем абсолютную погрешность каждого измерения
    абсолютные_погрешности = [abs(значение - среднее_значение) for значение in значения]

    # Вычисляем квадрат абсолютных погрешностей
    квадраты_погрешностей = [погрешность ** 2 for погрешность in абсолютные_погрешности]

    # Вычисляем среднеквадратичную погрешность среднего
    среднеквадратичная_погрешность = (sum(квадраты_погрешностей) / (len(значения)*(len(значения)-1))) ** 0.5

    # Вычисляем границы доверительного интервала (пусть будет 90% доверительный интервал)
    погрешность = 1.96 * среднеквадратичная_погрешность

    #Вычисляем емкость конденсатора или системы конденсаторов
    Емкость = Известная_Емкость * среднее_значение / Известное_число_делений

    #Вычислияем относительную погрешность емкости
    Относительная_Погрешность_Емкости = (((Погрешность_Известной_Емкости / Известная_Емкость) ** 2) + ((Погрешность_Известного_числа_делений / Известное_число_делений) ** 2) + ((погрешность/среднее_значение) ** 2)) ** 0.5

    #Погрешность Емкости
    погрешность_Емкости = Емкость * Относительная_Погрешность_Емкости

    # Выводим результаты
    print("Значения: ", значения)
    print("Среднее значение: ", среднее_значение)
    print("Абсолютные погрешности: ", абсолютные_погрешности)
    print("Квадраты абсолютных погрешностей: ", квадраты_погрешностей)
    print("Среднеквадратичная погрешность среднего: ", среднеквадратичная_погрешность)
    print("90% Доверительный интервал: ", среднее_значение ,"+-",погрешность)
    print("Емкость: ",Емкость)
    print("Относительная погрешность емкости: ",Относительная_Погрешность_Емкости)
    print("Погрешность емкости: ", погрешность_Емкости)
    print("Емкость Финальная: ",Емкость,"+-",погрешность_Емкости)

    print('\n')

Альтернативное_Последовательное = ((0.38333333333333336) * (0.353030303030303))/(0.38333333333333336 + 0.353030303030303)
Альтернативное_Параллельное = 0.38333333333333336 + 0.353030303030303

print(Альтернативное_Последовательное)
print(Альтернативное_Параллельное)
